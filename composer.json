{
    "name": "rueduphp/octo",
    "description": "Rue du PHP framework",
    "keywords": ["framework", "PHP7", "ORM", "rueduphp"],
    "license": "MIT",
    "authors": [
        {
            "name": "GÃ©rald Plusquellec",
            "email": "gplusquellec@rueduphp.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "fzaninotto/faker": "@stable",
        "aws/aws-sdk-php": "@stable",
        "illuminate/database": "*",
        "illuminate/redis": "*",
        "illuminate/view": "*",
        "illuminate/validation": "*",
        "laravel/socialite": "*",
        "wisembly/elephant.io": "~3.0",
        "nitmedia/wkhtml2pdf": "*",
        "twig/twig": "*",
        "intervention/image": "*",
        "intervention/imagecache": "*",
        "psy/psysh": "@stable",
        "predis/predis": "1.1.*@dev",
        "robmorgan/phinx": "@stable",
        "symfony/debug": "*",
        "symfony/process": "3.1.*",
        "filp/whoops": "^2.1",
        "psr/http-message": "^1.0",
        "guzzlehttp/psr7": "^1.4",
        "guzzlehttp/guzzle": "~6.0",
        "http-interop/response-sender": "^1.0",
        "psr/log": "~1.0",
        "swiftmailer/swiftmailer": "~5.1",
        "zendframework/zend-expressive-fastroute": "2.2",
        "http-interop/http-middleware": "^0.4",
        "middlewares/utils": "@stable",
        "willdurand/geocoder": "^3.3",
        "doctrine/dbal": "^2.6",
        "symfony/thanks": "^1.0",
        "ramsey/uuid": "^3.1",
        "elasticsearch/elasticsearch": "^6.0",
        "monolog/monolog": "~1.11",
        "mobiledetect/mobiledetectlib": "^2.8",
        "pagerfanta/pagerfanta": "^1.0",
        "algolia/algoliasearch-client-php": "^1.25",
        "zendframework/zendsearch": "dev-master",
        "mmanos/laravel-search": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0"
    },
    "autoload": {
        "files": [
            "lib/lib.php",
            "example/app/helpers.php"
        ],
        "psr-4": {
          "App\\": "example/app/"
        }
    },
    "autoload-dev": {
        "files": [
          "lib/lib.php"
        ],
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
          "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
